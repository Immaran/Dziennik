        <Label Grid.Row="0" FontSize="30" HorizontalAlignment="Center">Oceny</Label>
        <StackPanel Grid.Row="1" Name="StackPanel" HorizontalAlignment="Stretch"/>
        <!--<ListBox Name="GradeListBox" Grid.Row="1" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" VirtualizingPanel.ScrollUnit="Pixel"/>-->
    </Grid>
</Page>
