<Page x:Class="SBD.Pages.TeacherHome.HomeForTeacher"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SBD.Pages.TeacherHome"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HomeForTeacher" Loaded="Page_Loaded" Background="{StaticResource TeacherPageBackground}">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Label x:Name="mainLabel" FontSize="40"></Label>
            <Label x:Name="teacherLabel" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Center"></Label>
        </StackPanel>
    </Grid>
</Page>
