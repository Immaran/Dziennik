<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SBD.Styles.Global">
    
    <!--#region teacher-->

    <LinearGradientBrush x:Key="TeacherPageBackground" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#e6fff2" Offset="0"/>
        <GradientStop Color="#e6ffff" Offset="0.5"/>
        <GradientStop Color="#e6f2ff" Offset="1"/>
    </LinearGradientBrush >
    <LinearGradientBrush x:Key="TeacherBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="LightGreen" Offset="0"/>
        <GradientStop Color="LightBlue" Offset="1"/>
    </LinearGradientBrush >
    <SolidColorBrush x:Key="TeacherMouseOver" Color="SpringGreen"/>
    <SolidColorBrush x:Key="TeacherPressedBrush" Color="LightBlue"/>
    <!--#endregion teacher-->

</ResourceDictionary>